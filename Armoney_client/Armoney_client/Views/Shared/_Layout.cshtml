<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Armoney</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body>

    
    <div class="column">
        <div class="column right">
            <partial name="Login" />
            <br />
            <div>
                <h1>Site Map</h1>
                <p><a asp-area="" asp-controller="Money" asp-action="ProgressBar">ProgressBar</a></p>
                <p><a asp-area="" asp-controller="Money" asp-action="WhereIsMyMoney">Where Is My Money</a></p>
                <p><a asp-area="" asp-controller="Home" asp-action="CartView">Cart</a></p>
                <p><a asp-area="" asp-controller="Home" asp-action="AddProductView">Add product</a></p>
            </div>
        </div>
        </div>
        <div class="column left">
            @RenderBody()
        </div>
    </div>

    @*<div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>*@

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
